def get_valid_folder_path(prompt: str = "", allow_empty: bool = False) -> Optional[Path]:
    """
    é€šç”¨æ–‡ä»¶å¤¹è·¯å¾„è·å–å‡½æ•°
    
    Args:
        prompt: æç¤ºä¿¡æ¯
        allow_empty: æ˜¯å¦å…è®¸ç©ºè¾“å…¥
        
    Returns:
