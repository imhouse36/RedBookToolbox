    path_str = path_str.strip('\'"')  # ç§»é™¤å¼•å·
    folder_path = Path(path_str)
    
    # è·¯å¾„éªŒè¯
    if not folder_path.exists():
        raise ValueError(f"è·¯å¾„ä¸å­˜åœ¨: {path_str}")
    if not folder_path.is_dir():
        raise ValueError(f"ä¸æ˜¯æœ‰æ•ˆæ–‡ä»¶å¤¹: {path_str}")
    
    return folder_path

def scan_files_by_extensions(directory: Path, extensions: Set[str]) -> List[Path]:
    """
    é€’å½’æ‰«ææŒ‡å®šæ‰©å±•åæ–‡ä»¶
    
    Args:
        directory: ç›®æ ‡ç›®å½•
        extensions: æ–‡ä»¶æ‰©å±•åé›†åˆ (å¦‚ {'.jpg', '.png'})
        
    Returns:
        æ–‡ä»¶è·¯å¾„åˆ—è¡¨ï¼ŒæŒ‰åç§°æ’åº
    """
    files = []
    for ext in extensions:
        # å¤§å°å†™ä¸æ•æ„Ÿæœç´¢
        files.extend(directory.rglob(f"*{ext.lower()}"))
        files.extend(directory.rglob(f"*{ext.upper()}"))
    
    # å»é‡å¹¶æ’åº
    return sorted(list(set(files)))

def get_file_size_human(file_path: Path) -> str:
    """è·å–äººç±»å¯è¯»çš„æ–‡ä»¶å¤§å°"""
    size = file_path.stat().st_size
    for unit in ['B', 'KB', 'MB', 'GB', 'TB']:
        if size < 1024.0:
            return f"{size:.2f} {unit}"
        size /= 1024.0
    return f"{size:.2f} PB"

