import sys
import time
from typing import Dict, Any, Optional

class ProgressReporter:
    """ç»Ÿä¸€çš„è¿›åº¦æŠ¥å‘Šå·¥å…·"""
    
    def __init__(self, web_mode: Optional[bool] = None):
        if web_mode is None:
            self.web_mode = 'WEBP_TOOL_SERVER_MODE' in os.environ
        else:
            self.web_mode = web_mode
        
        self.start_time = time.time()
        self.last_update = 0
        
    def report_start(self, message: str):
        """æŠ¥å‘Šå¼€å§‹"""
        self._output(f"ğŸš€ {message}")
        
    def report_progress(self, current: int, total: int, item_name: str = ""):
        """æŠ¥å‘Šè¿›åº¦"""
        # é™åˆ¶æ›´æ–°é¢‘ç‡
        if time.time() - self.last_update < 0.5 and current < total:
            return
        self.last_update = time.time()
        
        percentage = (current / total * 100) if total > 0 else 0
        elapsed = time.time() - self.start_time
        
        message = f"ğŸ“Š è¿›åº¦: [{current}/{total}] {percentage:.1f}%"
        if item_name:
            message += f" - {item_name}"
        if elapsed > 1 and current > 0:
            eta = (elapsed / current * (total - current))
            message += f" (é¢„è®¡å‰©ä½™: {eta:.0f}ç§’)"
            
        self._output(message)
    
    def report_success(self, message: str):
        """æŠ¥å‘ŠæˆåŠŸ"""
        self._output(f"âœ… {message}")
    
    def report_error(self, message: str):
        """æŠ¥å‘Šé”™è¯¯"""
        self._output(f"âŒ {message}")
    
    def report_warning(self, message: str):
        """æŠ¥å‘Šè­¦å‘Š"""
        self._output(f"âš ï¸ {message}")
    
    def report_completion(self, stats: Dict[str, Any]):
        """æŠ¥å‘Šå®Œæˆç»Ÿè®¡"""
        elapsed = time.time() - self.start_time
        self._output("=" * 50)
        self._output("ğŸ“ˆ å¤„ç†å®Œæˆç»Ÿè®¡:")
        for key, value in stats.items():
            self._output(f"   {key}: {value}")
        self._output(f"â±ï¸ æ€»è€—æ—¶: {elapsed:.2f}ç§’")
        self._output("=" * 50)
    
    def _output(self, message: str):
        """ç»Ÿä¸€è¾“å‡ºå¤„ç†"""
        timestamp = time.strftime("%H:%M:%S")
        formatted_message = f"[{timestamp}] {message}"
        
        print(formatted_message, flush=True)
        
        # Webæ¨¡å¼ç¡®ä¿ç«‹å³åˆ·æ–°
        if self.web_mode:
            sys.stdout.flush()
